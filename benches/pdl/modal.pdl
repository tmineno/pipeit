# Modal benchmark - CSDF mode switching
# Tests control subgraph + mode transition semantics.

clock 10kHz adaptive {
    control {
        constant(0.0) | detect() -> ctrl
    }
    mode low {
        constant(0.0) | mul(1.0) | stdout()
    }
    mode high {
        constant(0.0) | mul(2.0) | stdout()
    }
    switch(ctrl, low, high) default low
}
